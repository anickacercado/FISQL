CREAR ROCEDIMIENTO poblarTablas () {
    CREAR OBJETO Mundo (INTEGER ciudades, INTEGER paises, INTEGER idiomas);

    DECLARAR @miMundo Mundo;
    @miMundo.ciudades = crearCiudades();
    @miMundo.paises = crearPaises ();
    @miMundo.idiomas = crearIdiomas ();

    SI(@miMundo.ciudades != 0) {
        IMPRIMIR("Ahora hay " + @ciudades + " filas en la tabla city. :-)");
    } SINO {
        IMPRIMIR(morir());
    }

    SI(@miMundo.paises != 0){
        IMPRIMIR("Ahora hay " + @paises + " filas en la tabla country. :-)");
    } SINO {
        IMPRIMIR(morir());
    }

    SI(@miMundo.idiomas != 0) {
        IMPRIMIR("Ahora hay " + @idiomas + " filas en la tabla country. :-)");
    } SINO {
        IMPRIMIR(morir());
    }
}

CREAR FUNCION morir () text {
     RETORNO "Algo salió mal, no te puedo seguir calificando... :-( ";
}

CREAR FUNCION sumar (INTEGER @a, INTEGER @b) INTEGER {
    RETORNO @a + @b;
}

CREAR FUNCION restar (INTEGER @a, INTEGER @b) INTEGER {
    DECLARAR @resultado = INTEGER;
    @resultado = @a - @b;
    RETORNO @resultado;
}

CREAR FUNCION multiplicar (INTEGER @a, INTEGER @b) INTEGER {
    RETORNO @a * @b;
}

CREAR FUNCION dividir (INTEGER @a, INTEGER @b) INTEGER {
    DECLARAR @resultado = INTEGER;
    @resultado = @a / @b;
    RETORNO @resultado;
}

CREAR FUNCION potencia (INTEGER @a, INTEGER @b) INTEGER {
    RETORNO @a ^ @b;
}

CREAR ROCEDIMIENTO consultarMundo(INTEGER @tipo) {
    SELECCIONA (@tipo) {
        CASO 100 :
            # Alternativa por si falla la condición CONTAR(<<SELECCIONAR * DE country>>)
            SI(@miMundo.paises != 0) {
                SELECCIONAR * 
                DE country;
            } SINO {
                IMPRIMIR(morir());
            }
            DETENER;
        CASO 200 :
             # Alternativa por si falla la condición CONTAR(<<SELECCIONAR * DE country>>) && CONTAR(<<SELECCIONAR * DE countrylanguage>>)
            SI(@miMundo.idiomas != 0 && @miMundo.paises != 0) {
                SELECCIONAR countrylanguage.countrycode, country.name, countrylanguage.language, countrylanguage.percentage
                DE countrylanguage, country 
                DONDE countrylanguage.countrycode == country.code && countrylanguage.isofficial == true 
                ORDENAR_POR country.code asc;
            } SINO {
                IMPRIMIR(morir());
            }
            DETENER;
        CASO 300 :
            # Alternativa por si falla la condición CONTAR(<<SELECCIONAR * DE country>>) && CONTAR(<<SELECCIONAR * DE countrylanguage>>)
            SI(@miMundo.idiomas != 0 && @miMundo.paises != 0) {
                SELECCIONAR countrylanguage.language, countrylanguage.percentage
                DE countrylanguage 
                DONDE countrylanguage.countrycode == 'GTM';
            } SINO {
                IMPRIMIR(morir());
            }
            DETENER;
        CASO 400 :
            # Alternativa por si falla la condición CONTAR(<<SELECCIONAR * DE country>>) && CONTAR(<<SELECCIONAR * DE city>>)
            SI(@miMundo.ciudades != 0 && @miMundo.paises != 0) {            
                SELECCIONAR id, name, district, population 
                DE city 
                DONDE countrycode == 'GTM' 
                ORDENAR_POR population desc;
            } SINO {
                IMPRIMIR(morir());
            }
            DETENER;
        CASO 500 :
            # Alternativa por si falla la condición CONTAR(<<SELECCIONAR * DE country>>) && CONTAR(<<SELECCIONAR * DE city>>)  && CONTAR(<<SELECCIONAR * DE countrylanguage>>)
            SI(@miMundo.idiomas != 0 && @miMundo.ciudades != 0 && @miMundo.paises != 0) {          
                SELECCIONAR country.population, country.code, country.name, city.name, countrylanguage.language  
                DE country, city, countrylanguage
                DONDE country.code == city.countrycode && country.code == countrylanguage.countrycode
                ORDENAR_POR country.population desc;

                DECLARAR @temporal INTEGER = CONTAR(<< SELECCIONAR * DE country ORDENAR_POR population desc>>);
                SI( @temporal > 10) {
                    DECLARAR @a INTEGER = 0;
                    MIENTRAS( @a < 10 ) {
                        IMPRIMIR("Solo se necesitan 10 de " + @temporal + " y vamos por el " + @a);
                        @a = @a + 1;
                    }
                }
            } SINO {
                IMPRIMIR(morir());
            }
            DETENER;
        CASO 600 :
            # Alternativa por si falla la condición CONTAR(<<SELECCIONAR * DE country>>) && CONTAR(<<SELECCIONAR * DE city>>)  && CONTAR(<<SELECCIONAR * DE countrylanguage>>)
            SI(@miMundo.idiomas != 0 && @miMundo.ciudades != 0 && @miMundo.paises != 0) {          
                SELECCIONAR country.population, countrylanguage.language
                DE country, city, countrylanguage
                DONDE country.code == city.countrycode && country.code == countrylanguage.countrycode
                ORDENAR_POR country.population desc;

                 IMPRIMIR("Solo tomar en cuenta los primeros 10");
            } SINO {
                IMPRIMIR(morir());
            }          
            DETENER;
        CASO 700 :
            # Alternativa por si falla la condición CONTAR(<<SELECCIONAR * DE country>>)
            SI(@miMundo.paises != 0) {
                SI( CONTAR(<< SELECCIONAR * DE country DONDE code = 'JPN'>>) != 0) {
                    ACTUALIZAR TABLA country (headofstate) VALORES ("Jimmy Morales") DONDE  code == 'JPN';
                }

                SI( CONTAR(<< SELECCIONAR * DE country DONDE code = 'JJJ'>>) == 0) {
                    IMPRIMIR("El pais no existe!!!!!");
                }       

                SELECCIONAR code, name, lifeexpectancy, headofstate 
                DE country 
                DONDE lifeexpectancy != nulo 
                ORDENAR_POR lifeexpectancy desc;

                DECLARAR @temporal INTEGER = CONTAR(<< SELECCIONAR * DE country DONDE lifeexpectancy != nulo ORDENAR_POR lifeexpectancy desc>>);
                SI( @temporal > 10) {
                    PARA( DECLARAR @a INTEGER = 0; @a < 10; ++){
                        IMPRIMIR("Solo se necesitan 10 de " + @temporal + " y vamos por el " + @a);
                    }
                }
            } SINO {
                IMPRIMIR(morir());
            }           
            DETENER;
        CASO 800 :
            # Alternativa por si falla la condición CONTAR(<<SELECCIONAR * DE country>>)
            SI(@miMundo.paises != 0) {
                IMPRIMIR("Actualizando la tabla de paises...");
                ALTERAR TABLA country AGREGAR (text voz_popular);

                SI( CONTAR(<< SELECCIONAR * DE country DONDE code = 'GTM'>>) != 0) {
                    ACTUALIZAR TABLA country (voz_popular) VALORES ('Jimmy se va!!!!!') DONDE  code == 'GTM';
                } SINO {
                    IMPRIMIR(morir() + ". Qué pasó, el país si debe existir...");
                }

                SELECCIONAR code, name, region, voz_popular 
                DE country 
                DONDE region == 'Central America' && code != 'MEX';
            } SINO {
                IMPRIMIR(morir());
            }
            DETENER;
        CASO 900 :
            # Alternativa por si falla la condición CONTAR(<<SELECCIONAR * DE country>>)
            SI(@miMundo.paises != 0) {
                SELECCIONAR code, name, region, voz_popular, population 
                DE country 
                DONDE region == 'Central America' && code != 'MEX' && code != 'BLZ' && code != 'PAN' && code != 'CRI' && code != 'NIC'
                ORDENAR_POR population desc ;

                ALTERAR OBJETO Mundo AGREGAR (text nombre);

                DECLARAR @otroMundo Mundo;
                @otroMundo.nombre = 'Tierra';

                IMPRIMIR("La " + @otroMundo.nombre + " tiene:");
                IMPRIMIR("Paises " + @otroMundo.paises);
                IMPRIMIR("Ciudades " + @otroMundo.ciudades);
                IMPRIMIR("Idiomas " + @otroMundo.idiomas);
            } SINO {
                IMPRIMIR(morir());
            }            
            DETENER;
        CASO 1000 :
            # Alternativa por si falla la condición CONTAR(<<SELECCIONAR * DE country>>)
            SI(@miMundo.paises != 0) {
                ACTUALIZAR TABLA country (continent) VALORES ('Europe') DONDE code == 'BRA' || code == 'GTM' || code == 'NIC' || code == 'SLV' || code == 'HND';
                
                SELECCIONAR code, name, lifeexpectancy 
                DE country 
                DONDE continent == 'Europe' 
                ORDENAR_POR lifeexpectancy asc;
            } SINO {
                IMPRIMIR(morir());
            } 
            DETENER;
        CASO 1100 :
            # Alternativa por si falla la condición CONTAR(<<SELECCIONAR * DE country>>)
            SI(@miMundo.paises != 0) {
                DECLARAR @cantidad INTEGER;
                @cantidad = CONTAR(<< SELECCIONAR * DE countrylanguage DONDE language == 'Spanish' && isofficial == 1 >>);
                IMPRIMIR('Los países donde el idioma español es el idioma oficial son: ' + @cantidad);
            } SINO {
                IMPRIMIR(morir());
            }
            DETENER;           
        CASO 1200 :
             # Alternativa por si falla la condición CONTAR(<<SELECCIONAR * DE country>>) && CONTAR(<<SELECCIONAR * DE city>>)  && CONTAR(<<SELECCIONAR * DE countrylanguage>>)
            SI(@miMundo.idiomas != 0 && @miMundo.ciudades != 0 && @miMundo.paises != 0) {
                IMPRIMIR("Eliminando columna voz_popular de la tabla country...");
                IMPRIMIR(FECHA_HORA());
                ALTERAR TABLA country QUITAR voz_popular;

                IMPRIMIR("Iniciando la destrucción de Africa...");
                IMPRIMIR(FECHA_HORA());
                IMPRIMIR("--------------------------------------------------------------------------");

                IMPRIMIR("Eliminando los idiomas de los países de Africa con mayor población...");
                IMPRIMIR(FECHA_HORA());
                BORRAR EN TABLA countrylanguage DONDE countrycode == 'DZA' || countrycode == 'EGY' || countrycode == 'ZAF' || countrycode == 'ETH' ||
                countrycode == 'COD' || countrycode == 'NGA' || countrycode == 'TZA';

                SI(CONTAR(<< SELECCIONAR * DE countrylanguage>>) == 56) {                  
                    IMPRIMIR('Los idimoas se eliminaron correctamente.');
                } SINO {
                    IMPRIMIR(morir() + ". No se eliminaron los idiomas de Africa que queríamos.");
                }

                IMPRIMIR("Eliminando los países de Africa con mayor población...");
                IMPRIMIR(FECHA_HORA());
                BORRAR EN TABLA country DONDE DONDE code == 'DZA' || code == 'EGY' || code == 'ZAF' || code == 'ETH' ||
                 code == 'COD' || code == 'NGA' || code == 'TZA';

                SI(CONTAR(<< SELECCIONAR * DE country>>) == 53) {                  
                    IMPRIMIR('Los países se eliminaron correctamente.');
                } SINO {
                    IMPRIMIR(morir() + ". No se eliminaron los países de Africa que queríamos.");
                }

                IMPRIMIR("Eliminando las ciudades de los países de Africa con mayor población...");
                IMPRIMIR(FECHA_HORA());
                BORRAR EN TABLA city DONDE countrycode == 'DZA' || countrycode == 'EGY' || countrycode == 'ZAF' || countrycode == 'ETH' ||
                countrycode == 'COD' || countrycode == 'NGA' || countrycode == 'TZA';

                SI(CONTAR(<< SELECCIONAR * DE city>>) == 51) {                  
                    IMPRIMIR('Las ciudades se eliminaron correctamente.');
                } SINO {
                    IMPRIMIR(morir() + ". No se eliminaron las ciudades de los países de Africa que queríamos.");
                }

                SI(CONTAR(<< SELECCIONAR * DE country DONDE continent == 'Africa'>>) == 3) {
                    SELECCIONAR * de country DONDE continent = 'Africa';
                } SINO {
                    IMPRIMIR(morir() + ". Algo salió mal al destruir Africa, corregir o morir...");
                }                                
            } SINO {
                IMPRIMIR(morir());
            }          
            DETENER;
        DEFECTO :
             IMPRIMIR("La opción no es válida... :-( ");
    }
}

